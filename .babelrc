{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
      [
        "babel-plugin-inline-import",
        {
          "extensions": [".svg"]
        }
      ],
      "babel-plugin-styled-components",
      [
        "module-resolver", {
          "root": ["./src"],
          "alias": {
            "components": "./src/components",
            "utils": "./src/common/utils",
            "constant": "./src/common/constant",
            "assets": "./src/assets",
            "screens": "./src/screens"
          }
        }
      ]
    ]
  }